{
	"cSpell.dictionaries": [
		"bash",
		"code-text-binary",
		"coding-compound-terms",
		"companies",
		"computing-acronyms",
		"cpp-legacy",
		"cpp-refined",
		"cpp",
		"csharp",
		"css",
		"data-science",
		"dotnet",
		"en_us", //godot appears to follow the programimg convention of prefering american spellings above british or canadian.
		"en-common-misspellings",
		"en-shared",
		"en-slang",
		"filetypes",
		"fonts",
		"fullstack",
		"game-development",
		"gamimg-terms",
		"git",
		"html-symbol-entities",
		"html",
		"lorem-ipsum",
		"markdown",
		"networking-terms",
		"people-names",
		"pokemon",
		"public-licenses",
		"python", //since gdscript and python share some overlaps
		"scientific-terms-gb",
		"scientific-terms-us",
		"shellscript",
		"software-term-suggestions",
		"software-tools",
		"vim",
		"web-services",
		"win32",
	],
	"cSpell.enabledFileTypes": {
		"*": true
	},
	"cSpell.ignorePaths": [
		".git/{index,*refs,*HEAD}",
		".git/{info,lfs,logs,refs,objects}/**",
		"*.import",
		"*.uid",
		"**/.gitkeep",
		"android/**",
	],
	"cSpell.ignoreRandomStrings": false,
	"cSpell.ignoreRegExpList": [
		//UID strings. These ids use 34 bit digits. IDK why, just how they were made.
		"uid://[a-y0-8]{1,13}",
		// The internal ids used in resource files
		"ExtResource\\([\"'][0-9a-zA-Z_]+[\"']\\)",
		"[\\[ ]id=[\"'][0-9a-zA-Z_]+[\"'][\\] ]",
		//Default names of gdscript resources local to scenes
		"GDScript_[a-y0-8]{1,5}", //These ids use 34 bit digits. IDK why, just how they were made.
		//Cspell has trouble with the utf acronyms
		"((UTF)|([uU]tf))([-_ ]?(8|(16)))?"
	],
	"cSpell.import": [
		"@cspell/dict-people-names/cspell-ext.json",
		"@cspell/dict-pokemon/cspell-ext.json",
		"@cspell/dict-scientific-terms-gb/cspell-ext.json",
		"@cspell/dict-scientific-terms-us/cspell-ext.json",
		"@cspell/dict-vim/cspell-ext.json",
		"@cspell/dict-win32/cspell-ext.json",
	],
	"cSpell.overrides": [
		{
			"filename": "SConstruct",
			"languageId": "python"
		}
	],
	"cSpell.spellCheckOnlyWorkspaceFiles": false,
	"cSpell.suggestWords": [
		"bbbcode:bbcode",
		"bbode:bbcode",
		"bcode:bbcode",
		"dg:gd",
		"dscript:gdscript",
		"gdfromat:gdformat",
		"gdilnt:gdlint",
		"gdot:godot",
		"gdscirpt:gdscript",
		"gdscrpt:gdscript",
		"gdt:godot gd",
		"godoot:godot",
		"godt:godot",
		"goodot:godot",
		"gscript:gdscript",
		"uft:utf",
		"novacd:NovaDC",
		"novadc:NovaDC",
	],
	"cSpell.unknownWords": "report-all",
	"cSpell.useGitignore": true,
	// "cSpell.allowCompoundWords": true, //some of godot's api sets off cspell, so enabling this might be usefull in some situations
	"cSpell.words": [
		"bb",
		"BBcode",
		"BBCode",
		"bbcode",
		"cfg",
		"cmdline",
		"ctex",
		"dblite",
		"deinit",
		"desynced",
		"elif",
		"eol",
		"gd",
		"GDExtention",
		"GDextention",
		"gdextention",
		"gdformat",
		"gdlint",
		"gdscript",
		"GDscript",
		"GDScript",
		"github",
		"Godot",
		"godot",
		"gui",
		"hdr",
		"idx",
		"init",
		"lfs",
		"lstrip",
		"premult",
		"res",
		"scn",
		"sconf",
		"scons",
		"SCons",
		"sconsign",
		"SConstuct",
		"sconstuct",
		"srgb",
		"tscn",
		"uid",
		"utc",
		"vram",
		"NovaDC",
		"novadc",
	],
	"godotFormatterAndLinter.lineLength": 100, // regardless of local prefrence, this is offically suggested
	"godotFormatterAndLinter.lintSeverityLevel": "Warning", // formatting errors are important here
	"godotTools.editor.revealTerminal": false
}
